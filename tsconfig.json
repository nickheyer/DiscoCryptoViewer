{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"lib": [
			"ES2022",
      "DOM"
		],
		"sourceMap": true,
		"rootDir": "src",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"allowJs": true,
		"esModuleInterop": true
	},
  "include": ["src/**/*"],
	"exclude": ["dist", "out", "node_modules", "webview"]
}
